{
    "name": "scayle/admin-api",
    "description": "SCAYLE Admin API SDK",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Scayle\\Cloud\\AdminApi\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scayle\\Cloud\\AdminApi\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
