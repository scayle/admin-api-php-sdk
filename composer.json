{
    "name": "aboutyou/admin-api",
    "description": "AboutYou Cloud AdminAPI SDK",
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AboutYou\\Cloud\\AdminApi\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AboutYou\\Cloud\\AdminApi\\": [
                "tests/"
            ]
        }
    }
}
